              </xsd:element>
              <xsd:element name="customFields" minOccurs="0" maxOccurs="1">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="customField1" minOccurs="0" maxOccurs="1" type="xsd:string"/>
