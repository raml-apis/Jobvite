          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="requiredFields" minOccurs="0" maxOccurs="1">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="applyLink" minOccurs="0" maxOccurs="1" type="xsd:string"/>
